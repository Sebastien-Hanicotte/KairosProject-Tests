{
	"name" : "kairos-project/tests",
	"description" : "A simple phpunit test helper",
	"type" : "library",
	"require" : {
		"php": "^7.2",
		"phpunit/phpunit" : "^8 | ^9@stable"
	},
	"require-dev": {
		"phpmd/phpmd": ">=2.6@stable",
		"squizlabs/php_codesniffer": ">=3.4@stable",
		"sebastian/phpcpd": ">=4@stable"
	},
	"license" : "MIT",
	"authors" : [{
			"name" : "matthieu88160",
			"email" : "matthieu.vallance@cscfa.fr"
		}
	],
	"minimum-stability" : "stable",
	"autoload" : {
		"psr-4" : {
			"KairosProject\\" : "KairosProject/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"KairosProject\\Tests\\Test\\": "Test/"
		}
	},
	"extra": {
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
	}
}
